{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Error",
      "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "None"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=Database;Port=5432;Database=productDB;User Id=postgres;Password=root",
    "ReadOnlyConnection": "Host=Database;Port=5432;Database=productDB;User Id=postgres;Password=root"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:8084"
      },
      "Https": {
        "Url": "https://+:443",
        "Certificate": {
          "Path": "/home/SSC.pfx",
          "Password": "Nhan123?"
        }
      }
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Elasticsearch" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Error",
        "Microsoft.Hosting.Lifetime": "Information",
        "Hangfire": "Error"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\log_.txt",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level}] {SourceContext} {NewLine} {Message:lj} {NewLine} {Exception} {NewLine}",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 1000000,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://SEQ:5341" }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://Elastic_Search:9200",
          "autoRegisterTemplate": true,
          "indexFormat": "product-service-logs-{0:yyyy.MM.dd}"
        }
      }
    ]
  },
  "AWS": {
    "Profile": "developer-nhan",
    "Region": "ap-southeast-1"
  }
}